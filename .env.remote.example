# Remote Database Environment Variables
# Copy this to .env.remote and configure with your actual remote database credentials
# Load before running migration: source .env.remote

# Remote Database Connection
export REMOTE_DB_HOST=your-remote-host.example.com
export REMOTE_DB_PORT=5432
export REMOTE_DB_NAME=eme_cache
export REMOTE_DB_USER=your_remote_user
